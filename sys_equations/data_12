hote_h <- function(t,pop , param_k){
  
  with(as.list(param_k), {return(list(c(
dX_1<- ak[1]*pop[1] - qk[1]*sum(pop)*pop[1] - bk[1]*pop[1] - betak[1]*pop[1]*sum(pop[2*1:12])+ gammak[1]*pop[2], dY_1<- betak[1]*pop[1]*sum(pop[2*1:12])- (alphak[1]+bk[1]+gammak[1])*pop[2],
dX_2<- ak[2]*pop[3] - qk[2]*sum(pop)*pop[3] - bk[2]*pop[3] - betak[2]*pop[3]*sum(pop[2*1:12])+ gammak[2]*pop[4], dY_2<- betak[2]*pop[3]*sum(pop[2*1:12])- (alphak[2]+bk[2]+gammak[2])*pop[4],
dX_3<- ak[3]*pop[5] - qk[3]*sum(pop)*pop[5] - bk[3]*pop[5] - betak[3]*pop[5]*sum(pop[2*1:12])+ gammak[3]*pop[6], dY_3<- betak[3]*pop[5]*sum(pop[2*1:12])- (alphak[3]+bk[3]+gammak[3])*pop[6],
dX_4<- ak[4]*pop[7] - qk[4]*sum(pop)*pop[7] - bk[4]*pop[7] - betak[4]*pop[7]*sum(pop[2*1:12])+ gammak[4]*pop[8], dY_4<- betak[4]*pop[7]*sum(pop[2*1:12])- (alphak[4]+bk[4]+gammak[4])*pop[8],
dX_5<- ak[5]*pop[9] - qk[5]*sum(pop)*pop[9] - bk[5]*pop[9] - betak[5]*pop[9]*sum(pop[2*1:12])+ gammak[5]*pop[10], dY_5<- betak[5]*pop[9]*sum(pop[2*1:12])- (alphak[5]+bk[5]+gammak[5])*pop[10],
dX_6<- ak[6]*pop[11] - qk[6]*sum(pop)*pop[11] - bk[6]*pop[11] - betak[6]*pop[11]*sum(pop[2*1:12])+ gammak[6]*pop[12], dY_6<- betak[6]*pop[11]*sum(pop[2*1:12])- (alphak[6]+bk[6]+gammak[6])*pop[12],
dX_7<- ak[7]*pop[13] - qk[7]*sum(pop)*pop[13] - bk[7]*pop[13] - betak[7]*pop[13]*sum(pop[2*1:12])+ gammak[7]*pop[14], dY_7<- betak[7]*pop[13]*sum(pop[2*1:12])- (alphak[7]+bk[7]+gammak[7])*pop[14],
dX_8<- ak[8]*pop[15] - qk[8]*sum(pop)*pop[15] - bk[8]*pop[15] - betak[8]*pop[15]*sum(pop[2*1:12])+ gammak[8]*pop[16], dY_8<- betak[8]*pop[15]*sum(pop[2*1:12])- (alphak[8]+bk[8]+gammak[8])*pop[16],
dX_9<- ak[9]*pop[17] - qk[9]*sum(pop)*pop[17] - bk[9]*pop[17] - betak[9]*pop[17]*sum(pop[2*1:12])+ gammak[9]*pop[18], dY_9<- betak[9]*pop[17]*sum(pop[2*1:12])- (alphak[9]+bk[9]+gammak[9])*pop[18],
dX_10<- ak[10]*pop[19] - qk[10]*sum(pop)*pop[19] - bk[10]*pop[19] - betak[10]*pop[19]*sum(pop[2*1:12])+ gammak[10]*pop[20], dY_10<- betak[10]*pop[19]*sum(pop[2*1:12])- (alphak[10]+bk[10]+gammak[10])*pop[20],
dX_11<- ak[11]*pop[21] - qk[11]*sum(pop)*pop[21] - bk[11]*pop[21] - betak[11]*pop[21]*sum(pop[2*1:12])+ gammak[11]*pop[22], dY_11<- betak[11]*pop[21]*sum(pop[2*1:12])- (alphak[11]+bk[11]+gammak[11])*pop[22],
dX_12<- ak[12]*pop[23] - qk[12]*sum(pop)*pop[23] - bk[12]*pop[23] - betak[12]*pop[23]*sum(pop[2*1:12])+ gammak[12]*pop[24], dY_12<- betak[12]*pop[23]*sum(pop[2*1:12])- (alphak[12]+bk[12]+gammak[12])*pop[24]
)))})}
